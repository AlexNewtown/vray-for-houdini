# V-Ray For Houdini Environment
#
# NOTE: This file is automatically generated by the build system!
#

export JAVA_HOME=/Library/Java/Home

export HOUDINI13_VOLUME_COMPATIBILITY=1
export HOUDINI_PYTHON_VERSION=2.6
export HOUDINI_DSO_ERROR=1

export HFS="@HOUDINI_FRAMEWORK_ROOT@/Resources"
export H="${HFS}"
export HB="${H}/bin"
export HDSO="${H}/../Libraries"
export HD="${H}/demo"
export HH="${H}/houdini"
export HHC="${HH}/config"
export HT="${H}/toolkit"
export HSB="${HH}/sbin"

export HOUDINI_MAJOR_RELEASE=@HOUDINI_MAJOR_RELEASE@
export HOUDINI_MINOR_RELEASE=@HOUDINI_MINOR_RELEASE@
export HOUDINI_BUILD_VERSION=@HOUDINI_VERSION_BUILD@
export HOUDINI_VERSION="${HOUDINI_MAJOR_RELEASE}.${HOUDINI_MINOR_RELEASE}.${HOUDINI_BUILD_VERSION}"

export VRAY_PLUGIN_DESC_PATH="@CMAKE_SOURCE_DIR@/deploy/plugins_desc"
export VRAY_SDK="@APPSDK_ROOT@"
export VRAY_PATH="${VRAY_SDK}/bin"
export VRAY_VFB_ALWAYS_ON_TOP=1

export DYLD_LIBRARY_PATH=${VRAY_SDK}/bin:@HOUDINI_HOME_PATH@/vfh/lib
export PATH="${HB}:${HSB}:${VRAY_PATH}:${PATH}"
