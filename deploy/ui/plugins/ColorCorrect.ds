	}
	parm {
	    name	"pre_brightness"
	    label	"Pre Brightness"
	    type	float
	    default	{ "0" }
	    help	"Added to the texture brightness"
	    disablewhen	"{ preprocess != 1 }"
	    range	{ 0 64 }
	    export	none
	    parmtag	{ "vray_plugin" "ColorCorrect" }
	    parmtag	{ "vray_pluginattr" "pre_brightness" }
