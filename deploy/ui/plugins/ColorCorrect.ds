{
    name	parameters
    parm {
	name	"plugindata"
	label	"Color Correct"
	type	data
	invisible
	default	{ "0" }
	help	"Color correct"
	range	{ 0 1 }
	export	none
	parmtag	{ "vray_category" "utility" }
	parmtag	{ "vray_plugin" "ColorCorrect" }
	parmtag	{ "vray_type" "TEXTURE" }
    }
    parm {
	name	"source_color"
	label	"Source Color"
	type	color4
	size	4
	default	{ "0" "0" "0" "0" }
	help	"Source color"
	range	{ 0 10 }
	export	none
	parmtag	{ "vray_plugin" "ColorCorrect" }
	parmtag	{ "vray_pluginattr" "source_color" }
	parmtag	{ "vray_type" "AColor" }
    }
    parm {
	name	"texture_map"
	label	"Texture Map"
	type	color4
	size	4
	default	{ "0" "0" "0" "1" }
	help	"The texture being color corrected"
	range	{ 0 10 }
	export	none
	parmtag	{ "vray_plugin" "ColorCorrect" }
	parmtag	{ "vray_pluginattr" "texture_map" }
	parmtag	{ "vray_type" "Texture" }
    }
    group {
	name	"folder"
	label	"RGB Pre-process"

