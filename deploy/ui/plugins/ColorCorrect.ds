		name	"pre_clamp_min"
		label	"Pre clamp min"
		type	float
		default	{ "0" }
		help	"low clamp value"
		disablewhen	"{ preprocess != 1 } { pre_clamp != 1 }"
		range	{ 0 64 }
		export	none
		parmtag	{ "vray_plugin" "ColorCorrect" }
		parmtag	{ "vray_pluginattr" "pre_clamp_min" }
		parmtag	{ "vray_type" "float" }
	    }
	    parm {
		name	"pre_clamp_max"
		label	"Pre clamp max"
		type	float
		default	{ "1" }
