		parmtag	{ "vray_plugin" "ColorCorrect" }
		parmtag	{ "vray_pluginattr" "preprocess" }
		parmtag	{ "vray_type" "boolean" }
	    }
	    parm {
		name	"pre_brightness"
		label	"Pre brightness"
		type	float
		default	{ "0" }
