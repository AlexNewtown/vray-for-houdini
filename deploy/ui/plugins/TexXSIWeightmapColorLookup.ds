	range	{ 0 1 }
	export	none
	parmtag	{ "vray_plugin" "TexXSIWeightmapColorLookup" }
	parmtag	{ "vray_type" "TEXTURE" }
    }
    parm {
	name	"channel"
	label	"Channel"
	type	integer
	default	{ "0" }
	help	"The channel number * 3 + a number indicating which of the x,y or z values are used"
	range	{ 0 10 }
	export	none
	parmtag	{ "cook_dependent" "1" }
	parmtag	{ "vray_plugin" "TexXSIWeightmapColorLookup" }
	parmtag	{ "vray_pluginattr" "channel" }
	parmtag	{ "vray_type" "int" }
    }
    parm {
	name	"factor"
