    parm {
	name	"weight_max"
	label	"Weight Max"
	type	float
	default	{ "1" }
	help	"Minimum weight (w/o factor)"
	range	{ 0 10 }
	export	none
	parmtag	{ "cook_dependent" "1" }
	parmtag	{ "vray_plugin" "TexXSIWeightmapColorLookup" }
	parmtag	{ "vray_pluginattr" "weight_max" }
	parmtag	{ "vray_type" "float" }
