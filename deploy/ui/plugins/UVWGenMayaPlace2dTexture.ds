        parmtag { "cook_dependent" "1" }
        parmtag { "vray_plugin" "UVWGenMayaPlace2dTexture" }
        parmtag { "vray_pluginattr" "repeat_v" }
        parmtag { "vray_type" "float" }
    }

    parm {
