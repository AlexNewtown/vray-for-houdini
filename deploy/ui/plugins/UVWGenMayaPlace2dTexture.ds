{
    name    UVWGenMayaPlace2dTexture
    label   "Channel (Named)"
    parmtag { spare_category "Channel (Named)" }
    parmtag { vray_plugin "UVWGenMayaPlace2dTexture" }

    parm {
        name    "uv_set_name"
        label   "UV Name"
        type    string
        default { "uv" }
        help    "The name of the UV channel that should be used"
        range   { 0 1 }
        export  none
        parmtag { "cook_dependent" "1" }
        parmtag { "vray_plugin" "UVWGenMayaPlace2dTexture" }
        parmtag { "vray_pluginattr" "uv_set_name" }
        parmtag { "vray_type" "string" }
    }

    parm {
        name    "uvw_channel"
        label   "UV Index"
        type    integer
        default { "0" }
        help    "Optional channel index, if uv_set_name is not found"
        range   { 0 64 }
        export  none
        parmtag { "cook_dependent" "1" }
        parmtag { "vray_plugin" "UVWGenMayaPlace2dTexture" }
        parmtag { "vray_pluginattr" "uvw_channel" }
        parmtag { "vray_type" "int" }
        disablewhen "{ uv_set_name != \"\" }"
    }

    parm {
        name    "uvw_channel_tex"
        label   "UV Channel"
        type    integer
        default { "0" }
        help    "Used when more than one mesh has UV linking specified for this 2d placement. If present will override uvw_channel"
        range   { 0 10 }
        export  none
        parmtag { "cook_dependent" "1" }
        parmtag { "vray_plugin" "UVWGenMayaPlace2dTexture" }
        parmtag { "vray_pluginattr" "uvw_channel_tex" }
        parmtag { "vray_type" "TextureInt" }
        invisible
    }

    parm {
        name    "repeat_u"
        label   "Repeat U"
        type    float
        default { "1" }
        range   { 0 10 }
        export  none
        parmtag { "cook_dependent" "1" }
        parmtag { "vray_plugin" "UVWGenMayaPlace2dTexture" }
        parmtag { "vray_pluginattr" "repeat_u" }
        parmtag { "vray_type" "float" }
    }

    parm {
        name    "repeat_v"
        label   "Repeat V"
        type    float
        default { "1" }
        range   { 0 10 }
