	parm {
	    name	"RadiancePart"
	    label	"Radiancepart"
	    type	float
	    default	{ "1" }
	    help	"Radiance % of base"
	    range	{ 0 10 }
	    export	none
	    parmtag	{ "vray_plugin" "TexBillboardParticle" }
	    parmtag	{ "vray_pluginattr" "RadiancePart" }
	    parmtag	{ "vray_type" "float" }
	}
	parm {
	    name	"ColourBurn"
	    label	"Colourburn"
	    type	float
	    default	{ "0" }
	    help	"Amount of colour burn to apply"
	    range	{ 0 10 }
	    export	none
	    parmtag	{ "vray_plugin" "TexBillboardParticle" }
	    parmtag	{ "vray_pluginattr" "ColourBurn" }
	    parmtag	{ "vray_type" "float" }
	}
    }

}
