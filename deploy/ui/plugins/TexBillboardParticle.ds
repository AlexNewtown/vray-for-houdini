	    type	float
	    default	{ "1" }
	    help	"Specular coeff"
	    range	{ 0 10 }
	    export	none
	    parmtag	{ "vray_plugin" "TexBillboardParticle" }
	    parmtag	{ "vray_pluginattr" "SpecularCoeff" }
	    parmtag	{ "vray_type" "float" }
	}
	parm {
	    name	"IrradianceType"
	    label	"Irradiancetype"
	    type	ordinal
	    default	{ "0" }
	    help	"Irradiance type (0 = none, 1 = colour, 2 = % of base "
	    menu	{
		"None"	"None"
		"Colour"	"Colour"
		"% of base"	"% of base"
	    }
	    range	{ 0 1 }
	    export	none
	    parmtag	{ "vray_enumkeys" "0,1,2" }
	    parmtag	{ "vray_plugin" "TexBillboardParticle" }
	    parmtag	{ "vray_pluginattr" "IrradianceType" }
	    parmtag	{ "vray_type" "int" }
	}
	parm {
	    name	"IrradianceClr"
	    label	"Irradianceclr"
	    type	color4
	    size	4
	    default	{ "0" "0" "0" "0" }
	    help	"Irradiance colour"
	    range	{ 0 10 }
	    export	none
	    parmtag	{ "vray_plugin" "TexBillboardParticle" }
	    parmtag	{ "vray_pluginattr" "IrradianceClr" }
	    parmtag	{ "vray_type" "Texture" }
	}
	parm {
	    name	"IrradiancePart"
	    label	"Irradiancepart"
	    type	float
	    default	{ "1" }
	    help	"Irradiance % of base"
	    range	{ 0 10 }
	    export	none
	    parmtag	{ "vray_plugin" "TexBillboardParticle" }
	    parmtag	{ "vray_pluginattr" "IrradiancePart" }
	    parmtag	{ "vray_type" "float" }
	}
	parm {
	    name	"RadianceType"
	    label	"Radiancetype"
	    type	ordinal
	    default	{ "0" }
	    help	"Radiance type (0 = none, 1 = colour, 2 = % of base "
	    menu	{
		"None"	"None"
		"Colour"	"Colour"
		"% of base"	"% of base"
	    }
	    range	{ 0 1 }
	    export	none
	    parmtag	{ "vray_enumkeys" "0,1,2" }
	    parmtag	{ "vray_plugin" "TexBillboardParticle" }
	    parmtag	{ "vray_pluginattr" "RadianceType" }
	    parmtag	{ "vray_type" "int" }
	}
	parm {
	    name	"RadianceClr"
	    label	"Radianceclr"
	    type	color4
	    size	4
	    default	{ "0" "0" "0" "0" }
	    help	"Radiance colour"
	    range	{ 0 10 }
	    export	none
	    parmtag	{ "vray_plugin" "TexBillboardParticle" }
	    parmtag	{ "vray_pluginattr" "RadianceClr" }
	    parmtag	{ "vray_type" "Texture" }
