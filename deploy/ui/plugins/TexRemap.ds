{
    name	parameters
    group {
	name	"TexRemap"
	label	"Remap"

	parm {
	    name	"plugindata"
	    label	"Remap"
	    type	data
	    invisible
	    default	{ "0" }
	    help	"Remap values"
	    range	{ 0 1 }
	    export	none
	    parmtag	{ "vray_category" "utility" }
	    parmtag	{ "vray_plugin" "TexRemap" }
	    parmtag	{ "vray_type" "TEXTURE" }
	}
	parm {
	    name	"input_value"
	    label	"Input Value"
	    type	float
	    default	{ "1" }
	    range	{ 0 64 }
	    export	none
	    parmtag	{ "vray_plugin" "TexRemap" }
	    parmtag	{ "vray_pluginattr" "input_value" }
	    parmtag	{ "vray_type" "TextureFloat" }
	}
	parm {
	    name	"input_color"
	    label	"Input Color"
	    type	color4
	    size	4
	    default	{ "0" "0" "0" "1" }
