	    name	"type"
	    label	"Type"
	    type	ordinal
	    default	{ "0" }
	    help	"type of conversion: 0:RemapValue, 1:RemapColor, 2:RemapHSV"
	    menu	{
		"0"	"RemapValue"
		"1"	"RemapColor"
		"2"	"RemapHSV"
	    }
