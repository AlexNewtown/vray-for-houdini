	type	float
	default	{ "0.25" }
	help	"Correction coefficient for surface expansion: from 0.0 to 1.0"
	range	{ 0 10 }
	export	none
	parmtag	{ "cook_dependent" "1" }
	parmtag	{ "vray_plugin" "GeomMetaballSystem" }
	parmtag	{ "vray_pluginattr" "smoothExpansion" }
	parmtag	{ "vray_type" "float" }
