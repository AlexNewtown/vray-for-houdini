	help	"Multiplier of the texture"
	range	{ 0 10 }
	export	none
	parmtag	{ "cook_dependent" "1" }
	parmtag	{ "vray_plugin" "GeomMetaballSystem" }
	parmtag	{ "vray_pluginattr" "textureMultiplier" }
	parmtag	{ "vray_type" "float" }
