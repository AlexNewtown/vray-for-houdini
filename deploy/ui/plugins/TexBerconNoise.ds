	    name	"noise_phase"
	    label	"Phase"
	    type	float
	    default	{ "0" }
	    help	"noise phase"
	    disablewhen	"{ noise_function != 2 noise_function != 5 }"
	    range	{ -1000 1000 }
	    export	none
	    parmtag	{ "vray_plugin" "TexBerconNoise" }
	    parmtag	{ "vray_pluginattr" "noise_phase" }
	    parmtag	{ "vray_type" "float" }
