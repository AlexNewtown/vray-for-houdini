	    help	"worley F2"
	    disablewhen	"{ noise_function != 6 }"
	    range	{ -1000 1000 }
	    export	none
	    parmtag	{ "vray_plugin" "TexBerconNoise" }
	    parmtag	{ "vray_pluginattr" "worley_F2" }
	    parmtag	{ "vray_type" "float" }
