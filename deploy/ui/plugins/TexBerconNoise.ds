    group {
	name	"folder5"
	label	"Noise Fractal"

	parm {
	    name	"fractal_type"
	    label	"Fractal"
	    type	ordinal
	    default	{ "0" }
	    help	"fractal type"
	    menu	{
		"0"	"None"
		"1"	"Fractal"
		"2"	"Turbulence"
		"3"	"fBm"
		"4"	"fBm Turbulence"
