	parm {
	    name	"low_tresh"
	    label	"Low"
	    type	float
	    default	{ "0" }
	    help	"default = 0.0f"
	    range	{ 0 1000 }
	    export	none
	    parmtag	{ "vray_plugin" "TexBerconNoise" }
	    parmtag	{ "vray_pluginattr" "low_tresh" }
	    parmtag	{ "vray_type" "float" }
	}
	parm {
	    name	"high_tresh"
	    label	"High"
	    type	float
	    default	{ "1" }
	    help	"default = 1.0f"
	    range	{ 0 1000 }
	    export	none
	    parmtag	{ "vray_plugin" "TexBerconNoise" }
	    parmtag	{ "vray_pluginattr" "high_tresh" }
	    parmtag	{ "vray_type" "float" }
	}
    }

    group {
	name	"folder4"
	label	"Noise Function"

	parm {
	    name	"uvw_dist"
	    label	"Uvw"
	    type	ordinal
	    default	{ "0" }
	    help	"UVW distance"
	    menu	{
		"0"	"Normal"
		"1"	"Radial"
		"2"	"Spherical"
	    }
	    range	{ 0 1 }
	    export	none
	    parmtag	{ "vray_plugin" "TexBerconNoise" }
	    parmtag	{ "vray_pluginattr" "uvw_dist" }
	    parmtag	{ "vray_type" "int" }
	}
	parm {
	    name	"noise_function"
	    label	"Noise Type"
	    type	ordinal
	    default	{ "0" }
	    help	"noise function"
	    menu	{
		"0"	"Perlin 2D"
		"1"	"Perlin 3D"
