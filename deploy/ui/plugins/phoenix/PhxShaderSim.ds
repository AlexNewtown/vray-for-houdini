	    export	none
	    parmtag	{ "vray_custom_handling" " " }
	    parmtag { "cook_dependent" "1" }
	    parmtag { "vray_plugin" "PhxShaderSim" }
	    parmtag { "vray_pluginattr" "renderMode" }
