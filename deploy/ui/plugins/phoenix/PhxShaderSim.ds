{
    name	selectedparms

    group {
	name	"folder4"
	label	"Rendering"

	parm {
	    name	"setPresetParam"
	    label	"Set this param to one of [FumeFX, HoudiniVolume, HoudiniLiquid, MayaFluids]"
	    type	string
	    invisible
	    default	{ "" }
	    range	{ 0 1 }
	    export	none
	}

	parm {
	    name	"selectedSopPath"
	    label	"Sop path to read data ranges from"
	    type	oppath
	    default	{ "" }
	    parmtag	{ "opfilter" "!!SOP!!" }
	    parmtag	{ "oprelative" "." }
	}

	parm {
	    name	"render"
	    label	"Enable Rendering"
	    type	toggle
	    default	{ "1" }
	    range	{ 0 1 }
	    export	none
	    parmtag { "cook_dependent" "1" }
	    parmtag { "vray_plugin" "PhxShaderSim" }
	    parmtag { "vray_pluginattr" "render" }
	    parmtag { "vray_type" "int" }
	}
	parm {
	    name	"jitter"
	    label	"Enable Jittering"
	    type	toggle
	    invisible
	    default	{ "1" }
	    range	{ 0 1 }
	    export	none
	    parmtag { "cook_dependent" "1" }
	    parmtag { "vray_plugin" "PhxShaderSim" }
	    parmtag { "vray_pluginattr" "jitter" }
	    parmtag { "vray_type" "int" }
	}
	parm {
	    name	"renderMode"
	    label	"Render Mode"
	    type	string
	    default	{ "0" }
	    menu	{
		"0"	"Volumetric"
		"1"	"Volumetric Geometry"
		"2"	"Volumetric Heat Haze"
		"3"	"Isosurface"
		"4"	"Mesh"
	    }
